{
	"Logging": {
		"LogLevel": {
			"Microsoft": "Warning",
			"Default": "Trace"
		},
		"Debug": {
			"LogLevel": {
				"Default": "Trace"
			}
		},
		"Console": {
			"IncludeScopes": false,
			"LogLevel": {
				"Default": "Trace",
				"Microsoft": "Warning"
			}
		},
		"Serilog": {
			"PathFormat": "../logs/log-{Date}.txt",
			"FileSizeLimitBytes": 10485760,
			"RetainedFileCountLimit": 10,
			"LogLevel": {
				"Default": "Trace",
				"Microsoft": "Warning"
			}
		}
	},
	"Images": {
		"SamplesPath": "./##/image samples"
	}
}
